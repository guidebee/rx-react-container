{
  "name": "rx-react-container",
  "version": "0.1.4",
  "author": "Bogdan Savluk <savluk.bogdan@gmail.com>",
  "description": "Allows to use React as a view layer for RxJS application, and to wait for required data before first rendering.",
  "keywords": [
    "react",
    "rx",
    "container",
    "isomorphic"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zxbodya/rx-react-container.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.12.0"
  },
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "lint": "eslint ./src/",
    "build": "rimraf lib && babel src --out-dir lib --source-maps",
    "cover": "jest --coverage",
    "test": "jest",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.20.0",
    "babel-jest": "^18.0.0",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "codecov.io": "^0.1.6",
    "enzyme": "^2.2.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "jest": "^18.0.0",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dom": "^15.4.1",
    "rimraf": "^2.4.4"
  },
  "peerDependencies": {
    "react": "*"
  },
  "dependencies": {
    "babel-runtime": "^6.20.0",
    "rx": "^4.0.7",
    "rx-combine-latest-obj": "^1.0.2"
  },
  "jest": {
    "testPathDirs": [
      "src"
    ]
  }
}
