{
  "name": "rx-react-container",
  "version": "0.2.1",
  "author": "Bogdan Savluk <savluk.bogdan@gmail.com>",
  "description": "Allows to use React as a view layer for RxJS application, and to wait for required data before first rendering.",
  "keywords": [
    "react",
    "rxjs",
    "container",
    "isomorphic"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zxbodya/rx-react-container.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.12.0"
  },
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "lint": "eslint ./src/",
    "build": "rimraf lib && babel src --out-dir lib --source-maps",
    "cover": "jest --coverage",
    "test": "jest",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "codecov.io": "^0.1.6",
    "enzyme": "^2.8.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^19.0.2",
    "react": "^15.5.3",
    "react-addons-test-utils": "^15.5.0",
    "react-dom": "^15.5.3",
    "rimraf": "^2.6.1"
  },
  "peerDependencies": {
    "react": "*"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "prop-types": "^15.5.4",
    "rxjs": "^5.3.0"
  },
  "jest": {
    "roots": [
      "src"
    ]
  }
}
